{
  "manifest_version": 3,
  "name": "X White",
  "version": "1.0",
  "description": "Automatically updates styles on x.com to white",
  "content_scripts": [
    {
      "matches": ["https://x.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "homepage_url": "https://github.com/Kino1994/x"
}
